@model IEnumerable<Sklep.Data.Data.CMS.WynikiIndyCar>
@{
    ViewData["Title"] = "Wyniki";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="card mb-4">
    <table class="table table-dark">
        <thead>

                <tr>
                    <td>Pozycja kierowcy</td>
                    <td>Imię i naziwsko</td>
                    <td>Zespół</td>
                    <td>Liczba punktów</td>
                </tr>
            
            @foreach(var wynikiIndyCar in Model)
            {
                <tr>
                    <td>@wynikiIndyCar.PozycjaKierowcy</td>
                    <td>@wynikiIndyCar.ImieNazwisko</td>
                    <td>@wynikiIndyCar.Zespol</td>
                    <td>@wynikiIndyCar.Punkty</td>
                </tr>
            }
        </thead>
    </table>
    </div> 